{
  "name": "@gratisvictory/stylelint",
  "version": "0.0.2",
  "description": "stylelint",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    ".stylelintrc.js"
  ],
  "keywords": [
    "stylelint"
  ],
  "author": "gratisvictory",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"*.js\""
  },
  "lint-staged": {
    "*.js": "prettier --write"
  },
  "peerDependencies": {
    "stylelint": "^16.0.2"
  },
  "dependencies": {
    "stylelint": "^16.0.2",
    "stylelint-config-idiomatic-order": "^10.0.0",
    "stylelint-config-standard-scss": "^12.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-config-tailwindcss": "0.0.7"
  },
  "devDependencies": {
    "@gratisvictory/prettier": "workspace:*"
    }
}
